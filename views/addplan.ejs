<%- include header %>
<%- include nav %>
<link rel="stylesheet" type="text/css" href="/stylesheets/mobiscroll.custom-2.6.2.min.css">
<div class="bs_container" id="bs_con_clock">
	<div class="bs_clock_dec">
		<p class="bs_clock_h1">&nbsp;&nbsp;Write down your plan, do your best to finish it.</p>
	</div>
	<div class="bs_one_block bs_clock_block">
		<!-- <div class="bs_one_block_title bs_clock_block_title">
			<p>TITLE</p>
		</div> -->
		<div class="bs_one_block_content bs_clock_addblock">
			<span class="bs_clock_be">PLAN</span>
			<input type="text" name="bs_activity" class="bs_input_text bs_clock_activity" placeholder="write your plan"><br>
			<span class="bs_clock_be">BEGIN TIME</span>
			<input type="text" name="bs_begintime" class="bs_input_text bs_clock_begintime" placeholder="click select time">
			<span class="bs_clock_be">END TIME</span>
			<input type="text" name="bs_endtime" class="bs_input_text bs_clock_endtime" placeholder="click select time"><span class="bs_clock_or">or</span>
			<input type="checkbox" name="bs_end_checkbox" class="bs_clock_end_checkbox"><span class="bs_clock_always">always</span>
			<input type="button" class="bs_input_button bs_clock_upload" value="SUBMIT"><span class="bs_l_msg"></span>
		</div>
	</div>
	<div class="bs_one_block bs_clock_block">
	</div>
	<div class="bs_one_block bs_clock_block">
	</div>
	<div class="bs_one_block bs_clock_block">
	</div>
</div>
