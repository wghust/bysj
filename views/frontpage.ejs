<%- include header %>
<section class="bs_frontpage">
	<div class="bs_welcome">
		<h1 class="bs_w_p1">WELCOME</h1>
		<div class="firstselect">
			<input type="button" class="bs_input_button" id="iamlearn" value="YOU ARE A LEARNER?"><br>
			<span id="selector">OR</span><br>
			<input type="button" class="bs_input_button" id="iamexaminee" value="YOU ARE AN EXAMINEE?"><br>
		</div>
		<p class="bs_w_p2"><input type="button" value="BEGIN" id="beginview"></p>
	</div>
	<div class="bs_f_op">
		<p class="bs_w_p1" id="bs_op_login">LOGIN</p>
		<p class="bs_w_p1" id="bs_op_reg">REGISTER</p>
		<input type="hidden" id="login" class="bs_op_hidden">
		<input type="text" placeholder="USERNAME" class="bs_u_username">
		<input type="text" placeholder="EMAIL" class="bs_u_email"><br>
		<input type="password" placeholder="PASSWORD" class="bs_u_password"><br>
		<input type="button" class="bs_login" value="login">
		<input type="button" class="bs_reg" value="reg"><br>
		<span class="bs_msg"></span>
	</div>
	<div class="bs_f_footer">
		<p class="bs_f_show">design by wbb</p>
	</div>
</section>
<script type="text/javascript">
	seajs.config({
		base:"/sea-modules/",
		alias:{
			"jquery":"jquery/jquery/1.10.1/jquery.js",
			"jquery-easing":"jquery/easing/1.3.0/easing.js"
		}
	});
	seajs.use("/javascripts/frontpage/src/frontpage");
</script>
<%- include footer %>